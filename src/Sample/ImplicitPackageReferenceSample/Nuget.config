<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <add key="repositoryPath" value="%NugetMachineInstallRoot%\.cbt" />
    <add key="globalPackagesFolder" value="%NugetMachineInstallRoot%\.cbt" />
  </config>
  <packageRestore>
    <add key="enabled" value="True" />
    <add key="automatic" value="True" />
  </packageRestore>
  <activePackageSource>
    <add key="All" value="(Aggregate source)" />
  </activePackageSource>
  <packageSources>
    <clear />
    <add key="Intune_Official" value="https://msazure.pkgs.visualstudio.com/_packaging/Intune_Official/nuget/v3/index.json" />
    <add key="Intune_External" value="https://msazure.pkgs.visualstudio.com/_packaging/Intune_External/nuget/v3/index.json" />
    <!-- Needed temporarily for pre-release packages. This should be removed once dependency packages are released. -->
    <add key="Intune_Dev" value="https://msazure.pkgs.visualstudio.com/_packaging/Intune_Dev/nuget/v3/index.json" />
    <add key="Intune_Volatile" value="https://msazure.pkgs.visualstudio.com/_packaging/Intune_Volatile/nuget/v3/index.json"/>
  </packageSources>
</configuration>