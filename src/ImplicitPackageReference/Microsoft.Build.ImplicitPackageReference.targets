<?xml version="1.0" encoding="utf-8" ?>
<Project>
  <UsingTask AssemblyFile="Microsoft.Build.ImplicitPackageReference.dll"
             TaskName="ImplicitPackageReferenceBuildTask"/>
  
  <Target Name="ImplicitPackageReferenceBuildTask" BeforeTargets="$(PackDependsOn)">
    <ImplicitPackageReferenceBuildTask AssetsFilePath="$(ProjectAssetsFile)" DependenciesToVersionAndPackage="@(ImplicitPackageReference)" Condition="@(ImplicitPackageReference)!= ''" />
  </Target>
</Project>

